{
  "name": "code",
  "displayName": "code",
  "description": "Link Script Syntax Highlighter",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configurationDefaults": {
      "[xo]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false
      }
    },
    "languages": [{
      "id": "link",
      "aliases": ["Link", "link"],
      "extensions": [".link"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "link",
      "scopeName": "source.link",
      "path": "./syntaxes/link.tmLanguage.json"
    }]
  }
}
