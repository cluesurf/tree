{
  "name": "xo-code",
  "displayName": "xo-code",
  "description": "Xo Syntax Highlighter",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configurationDefaults": {
      "[xo]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false
      }
    },
    "languages": [{
      "id": "xo",
      "aliases": ["Xo", "xo"],
      "extensions": [".xo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "xo",
      "scopeName": "source.xo",
      "path": "./syntaxes/xo.tmLanguage.json"
    }]
  }
}
